{
  "name": "@miaou/front",
  "version": "4.3.17",
  "author": "Thibaut Mottet",
  "description": "Front utilities for NuxtJs, Firebase",
  "license": "MIT",
  "module": "lib/*.js",
  "main": "lib/*.js",
  "typings": "lib/*.d.ts",
  "preserveModules": true,
  "private": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "yarn tsc",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "@miaou/types": "^1.0.11",
    "@nuxtjs/eslint-config": "12.0.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/gitlab": "9.5.0",
    "@semantic-release/npm": "9.0.1",
    "@typescript-eslint/parser": "5.44.0",
    "eslint": "8.28.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-nuxt": "4.0.0",
    "firebase": "^9.14.0",
    "semantic-release": "19.0.5",
    "typescript": "4.9.3",
    "vuex-class-component": "2.3.6",
    "dayjs": "^1.11.6"
  },
  "dependencies": {
    "pure-cache": "1.0.6"
  },
  "peerDependencies": {
    "firebase": "^9.14.0",
    "dayjs": "^1.11.6"
  }
}
